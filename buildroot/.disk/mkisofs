xorriso -as mkisofs -r -checksum_algorithm_iso md5,sha1 -V 'Debian 1.0 M-A 1' -o /data/SteamOS/ISO/cd-images/debian-1.0-amd64-i386-DVD-1.iso -J -isohybrid-mbr syslinux/usr/lib/syslinux/isohdpfx.bin -J -joliet-long -cache-inodes -b isolinux/isolinux.bin -c isolinux/boot.cat -no-emul-boot -boot-load-size 4 -boot-info-table -eltorito-alt-boot --efi-boot boot/grub/efi.img -append_partition 2 0x01 /data/SteamOS/ISO/tmp/alchemist/CD1/boot/grub/efi.img -partition_offset 16 boot1 CD1
